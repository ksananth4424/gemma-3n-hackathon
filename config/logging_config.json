{
  "level": "INFO",
  "log_to_file": true,
  "log_file_path": "logs/accessibility_assistant.log",
  "max_file_size_mb": 10,
  "backup_count": 5,
  "log_to_console": true,
  "console_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
  "use_json_format": false,
  "include_session_id": true,
  "log_performance_metrics": true,
  "log_sql_queries": false
}